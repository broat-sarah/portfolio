{
  "name": "my-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-p build:watch credentials",
    "build": "ng build",
    "test": "ng test --code-coverage",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "build:browser:prod": "ng build --prod",
    "build:browser:serverless": "ng build --prod --base-href /production/",
    "build:serverless": "npm run build:browser:serverless && npm run build:server:serverless",
    "build:prod": "npm run build:client-and-server-bundles && npm run webpack:server",
    "server": "node local.js",
    "build:prod:deploy": "npm run build:prod && npm run deploy",
    "build:serverless:deploy": "npm run build:serverless && npm run deploy",
    "deploy": "serverless deploy",
    "build:server:prod": "ng run myApp:server && webpack --config webpack.server.config.js --progress --colors",
    "build:server:serverless": "ng run myApp:server:serverless && webpack --config webpack.server.config.js --progress --colors",
    "postinstall": "node credentials.js && opencollective postinstall",
    "credentials": "node credentials.js",
    "build:watch": "ng serve",
    "build:dev": "run-p test:watch start",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:client-and-server-bundles": "ng build --prod && ng run myApp:server",
    "test:watch": "ng test --watch --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.3",
    "@angular/cdk": "^7.0.3",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "^7.0.3",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/platform-server": "^7.0.3",
    "@angular/router": "~7.0.0",
    "@angular/service-worker": "^7.0.3",
    "@mdi/font": "^3.0.39",
    "@nguniversal/express-engine": "^7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "@ngx-translate/core": "^11.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "angular-materialize": "^0.2.2",
    "angular-typing-animation": "^0.5.4",
    "aws-serverless-express": "^3.2.0",
    "core-js": "^2.5.4",
    "cors": "~2.8.4",
    "cp-cli": "^1.1.0",
    "ngx-materialize": "^6.1.3",
    "rxjs": "~6.3.3",
    "rxjs-compat": "^6.3.3",
    "typescript-collections": "^1.3.2",
    "webpack-cli": "3.1.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.3",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.3",
    "codelyzer": "~4.5.0",
    "cp-cli": "^1.1.0",
    "cpy-cli": "^2.0.0",
    "decompress": "^4.2.0",
    "decompress-targz": "^4.1.1",
    "express": "^4.15.2",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "node-sass": "^4.10.0",
    "node-wget": "^0.4.2",
    "npm-run-all": "^4.1.2",
    "opencollective": "^1.0.3",
    "pre-commit": "^1.2.2",
    "protractor": "~5.4.0",
    "reflect-metadata": "^0.1.10",
    "serverless": "^1.32.0",
    "serverless-apigw-binary": "^0.4.4",
    "sinon": "^7.1.1",
    "ts-loader": "5.3.0",
    "ts-node": "~7.0.0",
    "tslint": "^5.7.0",
    "typescript": "~3.1.1"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/ng-toolkit"
  }
}
